{
	"appenders": {
		"consolelog": {
			"type": "console"
		},
		"fileLog": {
			"type": "file",
			"filename": "/var/log/web-ssh/server.log"
		},
		"accessLog": {
			"type": "file",
			"filename": "/var/log/web-ssh/access.log"
		}
	},
	"categories": {
		"default": {
			"level": "debug",
			"appenders": [
				"fileLog",
				"consolelog"
			]
		},
		"access": {
			"level": "info",
			"appenders": [
				"accessLog"
			]
		},
		"HTTP": {
			"level": "info",
			"appenders": [
				"accessLog"
			]
		}
	}
}
